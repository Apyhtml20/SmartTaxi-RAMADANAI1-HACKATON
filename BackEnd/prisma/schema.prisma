generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

enum Role {
  DRIVER
  PASSENGER
}

model User {
  id           Int    @id @default(autoincrement())
  fullName     String
  email        String @unique
  passwordHash String
  role         Role
  createdAt    DateTime @default(now())
}
